<script>
          const sendData = async (event, formob, action) => {
          event.preventDefault();

          let obj = {};
          let formData = new FormData(formob);
          for (let item of formData.entries()) {
            obj[item[0]] = item[1];
          }
          obj.oemail = "<%=data.cpemail%>";
          formob.reset();
          const res = await fetch('/profile/'.concat(action), {
            method: 'post',
            headers: {
              'content-type': 'application/json'
            },
            body: JSON.stringify(obj)
          });

          const json = await res.json();
         
          alert(json.msg);
        }
</script>
